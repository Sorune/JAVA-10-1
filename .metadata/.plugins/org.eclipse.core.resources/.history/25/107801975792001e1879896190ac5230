package ch05;

import java.util.*;

public class TimerExam {
	public static void main(String[] args) {
		Date date = new Date();
		int count = 0;
		long now = date.getTime(), time = date.getTime();
		while (true) {
			now = date.getTime();
			long remain = now - time;
			
			System.out.println(remain);
		}
	}
}
