package ch06;

import java.util.Scanner;

public class ReservationSystem {
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int count = 100;
		for (int i = 0 ; i < 30 ; i++) {
			System.out.printf("[ %20d. %02d ]",i+1,i+count);
			if (i%5 == 0) {
				System.out.println();
				count+=100;
			}
		}
	}
}
