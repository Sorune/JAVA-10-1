package ch07;

public class Member {
	public String name;
	public int age;
	public String id;
	public String pass;
	public String phone;
	public String adress;
	
	public Member() {
		
	}
	
	public void checkAge() {
		if (age > 20) {
			System.out.println("성인입니다.");
		}
		else {
			System.out.println("성인이 아닙니다.");
		}
	}
	
}
