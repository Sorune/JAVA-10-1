package ch10.trycatch;

public class MultiCatchExam {
	public static void main(String[] args) {
		//catch 블록은 여러개 가능
		
		String data1 = null;
		String data2 = null;
		data1 = args[0];
		data2 = args[1];
		//argments 값이 2개가 아닐때
		
		int value1 = Integer.parseInt(data1);
		int value2 = Integer.parseInt(data2);
		
	}
}
