package ch06;

import java.util.Scanner;

public class ReservationSystem {
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		
		
		
		int count = 100,roomNum = 1;;
		for (int i = 0 ; i < 30 ; i++) {
			roomNum += i;
			System.out.printf("[ %02d. %02d ]",i+1,roomNum+count);
			if ((i+1)%5 == 0) {
				System.out.println();
				count+=100;
				roomNum = 1;
			}
		}
	}
}
