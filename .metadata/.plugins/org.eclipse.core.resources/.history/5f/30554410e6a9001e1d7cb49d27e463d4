package ch12.share;

public class ShareExam {
	public static void main(String[] args) {
		Calculator cal = new Calculator();
		Calculator cal2 = cal;
		User1 user1 = new User1();
		User2 user2 = new User2();
		
		user1.setCalculator(cal);
		user1.start();
		
		user2.setCalculator(cal2);
		user2.start();
	}
}
