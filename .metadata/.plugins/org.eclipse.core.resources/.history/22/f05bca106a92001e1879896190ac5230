package ch05;

public class BreakOutterExam {
	public static void main(String[] args) {
		Outter : for (char upper = 'A' ; upper <= 'Z'; upper++) {
			for (char lower = 'a' ; lower <= 'z' ; lower++) {
				if (lower == 'c') {
					System.out.printf("%s-%s\n",upper,lower);
					break Outter;
				}
			}
				
		}
	}
}
